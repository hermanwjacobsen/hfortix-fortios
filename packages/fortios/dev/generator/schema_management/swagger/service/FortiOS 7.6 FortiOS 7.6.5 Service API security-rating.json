                                                        "properties": {
                                                            "structure": {
                                                                "title": "Recommendation Structure",
                                                                "description": "Structure that defines the elements for the Security Rating check recommendation.",
                                                                "type": "array",
                                                                "items": {
                                                                    "$ref": "#\/definitions\/element"
                                                                }
                                                            },
                                                            "tables": {
                                                                "title": "CMDB Tables",
                                                                "description": "CMDB tables that this recommendation affects.",
                                                                "type": "array",
                                                                "items": {
                                                                    "title": "CMDB Table",
                                                                    "description": "CMDB table that is affected by this recommendation.",
                                                                    "type": "string"
                                                                }
                                                            }
                                                        }
                                                    }

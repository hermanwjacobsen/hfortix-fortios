        "\/security-rating\/report\/": {
            "get": {
                "summary": "Retrieve full report of all Security Rating tests. \n Access Group: secfabgrp.csfsys",
                "tags": [
                    "security-rating"
                ],
                "responses": {
                    "400": {
                        "description": "Bad Request: Request cannot be processed by the API"
                    },
                    "401": {
                        "description": "Not Authorized: Request without successful login session"
                    },
                    "403": {
                        "description": "Forbidden: Request is missing CSRF token or administrator is missing access profile permissions."
                    },
                    "404": {
                        "description": "Resource Not Found: Unable to find the specified resource."
                    },
                    "405": {
                        "description": "Method Not Allowed: Specified HTTP method is not allowed for this resource. "
                    },
                    "424": {
                        "description": "Failed Dependency: Fail dependency can be duplicate resource, missing required parameter, missing required attribute, invalid attribute value"
                    },
                    "200": {
                        "description": "OK: Request returns successful",
                        "schema": {
                            "title": "Security Rating Report",
                            "description": "Report for a given Security Rating subtype.",
                            "type": "array",
                            "items": {
                                "title": "Security Rating Check",
                                "description": "Results for a Security Rating check.",
                                "type": "object",
                                "required": [
                                    "check",
                                    "title",
                                    "description",
                                    "severity"
                                ],
                                "properties": {
                                    "check": {
                                        "title": "Check",
                                        "description": "ID of the Security Rating check.",
                                        "type": "string"
                                    },
                                    "title": {
                                        "title": "Title",
                                        "description": "Translation string for the title of the Security Rating check.",
                                        "type": "string"
                                    },
                                    "description": {
                                        "title": "Description",
                                        "description": "Translation string for the description of the Security Rating check.",
                                        "type": "string"
                                    },
                                    "severity": {
                                        "title": "Severity",
                                        "description": "Severity of the Security Rating check.",
                                        "type": "string",
                                        "enum": [
                                            "none",
                                            "low",
                                            "medium",
                                            "high",
                                            "critical"
                                        ]
                                    },
                                    "customMetadata": {
                                        "title": "Custom Metadata",
                                        "description": "Custom metadata for the Security Rating check.",
                                        "type": "object",
                                        "additionalProperties": true
                                    },
                                    "summary": {
                                        "title": "Summary",
                                        "description": "Summary details of the Security Rating check results.",
                                        "type": "array",
                                        "items": {
                                            "title": "Summary Details",
                                            "description": "Summary details for a single Security Fabric device and domain.",
                                            "type": "object",
                                            "required": [
                                                "device",
                                                "deviceType",
                                                "vdom",
                                                "scope"
                                            ],
                                            "properties": {
                                                "device": {
                                                    "title": "Device Serial",
                                                    "description": "Serial number of the device.",
                                                    "type": "string"
                                                },
                                                "deviceType": {
                                                    "title": "Device Type",
                                                    "description": "The device type.",
                                                    "type": "string"
                                                },
                                                "vdom": {
                                                    "title": "VDOM",
                                                    "description": "VDOM.",
                                                    "type": "string"
                                                },
                                                "scope": {
                                                    "title": "Scope",
                                                    "description": "Scope.",
                                                    "type": "string",
                                                    "enum": [
                                                        "vdom",
                                                        "global"
                                                    ]
                                                },
                                                "issueCount": {
                                                    "title": "Issue Count",
                                                    "description": "Number of issues for this check on this device and VDOM.",
                                                    "type": "integer"
                                                },
                                                "result": {
                                                    "title": "Result",
                                                    "description": "Result of this check on this device and VDOM.",
                                                    "type": "string",
                                                    "enum": [
                                                        "passed",
                                                        "failed",
                                                        "exempt",
                                                        "unmetDependencies",
                                                        "error"
                                                    ]
                                                },
                                                "timestamp": {
                                                    "title": "Last Ran",
                                                    "description": "UNIX epoch timestamp of the last time this check was ran for this device and VDOM.",
                                                    "type": "integer"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error: Internal error when processing the request "
                    },
                    "413": {
                        "description": "Request Entity Too Large: Request cannot be processed due to large entity "
                    },
                    "429": {
                        "description": "Access temporarily blocked: Maximum failed authentications reached. The offended source is temporarily blocked for certain amount of time."
                    }
                },
                "parameters": [
                    {
                        "name": "scope",
                        "in": "query",
                        "description": "Scope of the request [global | vdom*].",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "standalone",
                        "in": "query",
                        "description": "If enabled this will only return a report with checks for the current FortiGate.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "description": "The report sub-type to fetch ['psirt', 'insight'].",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "checks",
                        "in": "query",
                        "description": "Retrieve a report with only the given Security Rating checks.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "show-hidden",
                        "in": "query",
                        "description": "Show hidden Security Rating controls in the report.",
                        "required": false,
                        "type": "string"
                    }
                ]
            }
        },
        "\/security-rating\/recommendations\/": {

                                            "title": "Device Recommendation Overview",
                                            "description": "Overview on Security Rating recommendations for a given device.",
                                            "type": "object",
                                            "required": [
                                                "device",
                                                "deviceType",
                                                "vdom",
                                                "scope"
                                            ],
                                            "properties": {
                                                "device": {
                                                    "title": "Device Serial",
                                                    "description": "Serial number of the device.",
                                                    "type": "string"
                                                },
                                                "deviceType": {
                                                    "title": "Device Type",
                                                    "description": "The device type.",
                                                    "type": "string"
                                                },
                                                "vdom": {
                                                    "title": "Device VDOM",
                                                    "description": "VDOM that the recommendation is for.",
                                                    "type": "string"
                                                },
                                                "scope": {
                                                    "title": "Device Scope",

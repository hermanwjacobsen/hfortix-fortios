    "paths": {
        "\/extension-controller\/fortigate": {
            "get": {
                "tags": [
                    "extension-controller"
                ],
                "summary": "Retrieve statistics for configured FortiGate LAN Extension Connectors.\nAccess Group: netgrp.cfg",
                "responses": {
                    "200": {
                        "description": "OK: Request returns successful",
                        "schema": {
                            "title": "FortiGate Connector Information",
                            "description": "Retrieve detailed information for FortiGate Connector(s).",
                            "type": "array",
                            "items": {
                                "title": "FortiGate Connector Details",
                                "description": "Details for a FortiGate Connector.",
                                "type": "object",
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "title": "Name",
                                        "description": "FortiGate Connector name."
                                    },
                                    "ip": {
                                        "type": "string",
                                        "title": "IP Address",
                                        "description": "IP address of FortiGate Connector."
                                    },
                                    "status": {
                                        "type": "string",
                                        "title": "Status",
                                        "description": "FortiGate Connector status.",
                                        "enum": [
                                            "running",
                                            "pending"
                                        ]
                                    },
                                    "uptime": {
                                        "type": "number",
                                        "title": "Uptime",
                                        "description": "Length of time the device has been up, in seconds."
                                    },
                                    "port": {
                                        "title": "Port",
                                        "description": "Port of the connection.",
                                        "type": "number"
                                    },
                                    "authorization_status_locked": {
                                        "title": "Authorization Status Locked",
                                        "description": "True if the authorization status for FortiGate Connector cannot be changed.",
                                        "type": "boolean"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request: Request cannot be processed by the API"
                    },
                    "401": {
                        "description": "Not Authorized: Request without successful login session"
                    },
                    "403": {
                        "description": "Forbidden: Request is missing CSRF token or administrator is missing access profile permissions."
                    },
                    "404": {
                        "description": "Resource Not Found: Unable to find the specified resource."
                    },
                    "405": {
                        "description": "Method Not Allowed: Specified HTTP method is not allowed for this resource. "
                    },
                    "413": {
                        "description": "Request Entity Too Large: Request cannot be processed due to large entity "
                    },
                    "424": {
                        "description": "Failed Dependency: Fail dependency can be duplicate resource, missing required parameter, missing required attribute, invalid attribute value"
                    },
                    "429": {
                        "description": "Access temporarily blocked: Maximum failed authentications reached. The offended source is temporarily blocked for certain amount of time."
                    },
                    "500": {
                        "description": "Internal Server Error: Internal error when processing the request "
                    }
                }
            }
        },
        "\/extension-controller\/lan-extension-vdom-status": {
            "get": {
                "tags": [
                    "extension-controller"
                ],
                "summary": "Retrieve information for the FortiGate LAN Extension VDOM.\nAccess Group: netgrp.cfg",
                "responses": {
                    "200": {
                        "description": "OK: Request returns successful",
                        "schema": {
                            "title": "LAN Extension VDOM Status Information",
                            "description": "Retrieve detailed information for LAN Extension VDOM Status.",
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string",
                                    "title": "Name",
                                    "description": "FortiGate Controller name."
                                },
                                "ip": {
                                    "type": "string",
                                    "title": "IP Address",
                                    "description": "IP address of FortiGate Controller."
                                },
                                "status": {
                                    "type": "string",
                                    "title": "Status",
                                    "description": "Status of the LAN Extension connection.",
                                    "enum": [
                                        "EXTWS_RUN",
                                        "EXTWS_SULKING",
                                        "EXTWS_JOIN",
                                        "EXTWS_DISCOVERY",
                                        "EXTWS_DTLS_SETUP",
                                        "EXTWS_IDLE"
                                    ]
                                },
                                "uptime": {
                                    "type": "number",
                                    "title": "Uptime",
                                    "description": "Length of time the device has been up, in seconds."
                                },
                                "port": {
                                    "type": "number",
                                    "title": "Port",
                                    "description": "Port of the service."
                                },
                                "uplink": {
                                    "type": "array",
                                    "title": "Uplink List",
                                    "description": "List of the connecting ports.",
                                    "items": {
                                        "title": "Uplink",
                                        "description": "Name of the connecting port.",
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request: Request cannot be processed by the API"
                    },
                    "401": {
                        "description": "Not Authorized: Request without successful login session"
                    },
                    "403": {
                        "description": "Forbidden: Request is missing CSRF token or administrator is missing access profile permissions."
                    },
                    "404": {
                        "description": "Resource Not Found: Unable to find the specified resource."
                    },
                    "405": {
                        "description": "Method Not Allowed: Specified HTTP method is not allowed for this resource. "
                    },
                    "413": {
                        "description": "Request Entity Too Large: Request cannot be processed due to large entity "
                    },
                    "424": {
                        "description": "Failed Dependency: Fail dependency can be duplicate resource, missing required parameter, missing required attribute, invalid attribute value"
                    },
                    "429": {
                        "description": "Access temporarily blocked: Maximum failed authentications reached. The offended source is temporarily blocked for certain amount of time."
                    },
                    "500": {
                        "description": "Internal Server Error: Internal error when processing the request "
                    }
                }
            }
        }
    }
}
{
    "swagger": "2.0",
    "info": {
        "title": "FortiOS Monitor",
        "description": "REST API for configuring FortiOS objects and settings",
        "version": "v2",
        "termsOfService": "http:\/\/fortinet.com\/terms\/",
        "contact": {
            "name": "FortiOS Web Application Team",
            "email": "fosguiqateam@fortinet.com",
            "url": "http:\/\/modskristensen.net"
        }
    },
    "host": "172.16.177.65",
    "basePath": "\/api\/v2\/monitor",
    "schemes": [
        "https"
    ],
    "consumes": [
        "application\/json"
    ],
    "produces": [
        "application\/json"
    ],
    "securityDefinitions": {
        "APIKeyQueryParam": {
            "type": "apiKey",
            "in": "query",
            "name": "access_token"
        }
    },
    "security": [
        {
            "APIKeyQueryParam": []
        }
    ],
    "paths": {
        "\/casb\/saas-application\/details": {
            "get": {
                "tags": [
                    "casb"
                ],
                "summary": "Retrieve details for CASB SaaS applications.\nAccess Group: utmgrp.casb",
                "responses": {
                    "200": {
                        "description": "OK: Request returns successful",
                        "schema": {
                            "title": "SaaS Application Package Details",
                            "description": "List of the predefined SaaS Applications with the matching domains.",
                            "type": "array",
                            "items": {
                                "title": "SaaS Application Predefined Details Item",
                                "type": "object",
                                "description": "SaaS application details.",
                                "required": [
                                    "name",
                                    "domains",
                                    "icon_id",
                                    "casb_display_name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "title": "SaaS Application Name",
                                        "description": "Name of the SaaS application.",
                                        "examples": [
                                            "youtube"
                                        ]
                                    },
                                    "icon_id": {
                                        "type": "number",
                                        "title": "ID of Application Icon",
                                        "description": "SaaS Application Icon ID for icon map.",
                                        "examples": [
                                            1,
                                            2,
                                            3
                                        ]
                                    },
                                    "casb_display_name": {
                                        "type": "string",
                                        "title": "CASB Display Name",
                                        "description": "CASB Display Name, use this for translations and in the GUI.",
                                        "examples": [
                                            "Youtube"
                                        ]
                                    },
                                    "domains": {
                                        "title": "Domains",
                                        "type": "array",
                                        "description": "Domains of the SaaS Application.",
                                        "items": {
                                            "title": "Domain Item",
                                            "description": "Domain used to match the SaaS application.",
                                            "type": "string",
                                            "examples": [
                                                "youtube.com"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request: Request cannot be processed by the API"
                    },
                    "401": {
                        "description": "Not Authorized: Request without successful login session"
                    },
                    "403": {
                        "description": "Forbidden: Request is missing CSRF token or administrator is missing access profile permissions."
                    },
                    "404": {
                        "description": "Resource Not Found: Unable to find the specified resource."
                    },
                    "405": {
                        "description": "Method Not Allowed: Specified HTTP method is not allowed for this resource. "
                    },
                    "413": {
                        "description": "Request Entity Too Large: Request cannot be processed due to large entity "
                    },
                    "424": {
                        "description": "Failed Dependency: Fail dependency can be duplicate resource, missing required parameter, missing required attribute, invalid attribute value"
                    },
                    "429": {
                        "description": "Access temporarily blocked: Maximum failed authentications reached. The offended source is temporarily blocked for certain amount of time."
                    },
                    "500": {
                        "description": "Internal Server Error: Internal error when processing the request "
                    }
                },
                "parameters": [
                    {
                        "name": "mkey",
                        "in": "query",
                        "description": "Filter: Key of the application to be fetched",
                        "required": false,
                        "type": "string"
                    }
                ]
            }
        }
    }
}
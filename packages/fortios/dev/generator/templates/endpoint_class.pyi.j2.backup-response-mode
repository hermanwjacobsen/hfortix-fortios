{% endif %}
{% endif %}
    
    # raw_json=True returns the full API envelope
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }} | None = ...,

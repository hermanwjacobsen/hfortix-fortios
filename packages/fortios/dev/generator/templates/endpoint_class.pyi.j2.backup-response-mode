        action: str | None = ...,
        vdom: str | bool | None = ...,
        raw_json: bool = ...,
        *,
        response_mode: Literal["dict"],
        **kwargs: Any,
{% if schema.fields %}
{% if schema.mkey %}
    ) -> list[{{ schema.class_name }}Response]: ...
{% else %}
    ) -> {{ schema.class_name }}Response: ...
{% endif %}
{% else %}
{% if schema.mkey %}

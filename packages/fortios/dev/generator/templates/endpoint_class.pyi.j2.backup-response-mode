class {{ schema.class_name }}DictMode:
    """{{ schema.class_name }} endpoint for dict response mode (default for this client).
    
    By default returns {{ schema.class_name }}Response (TypedDict).
    Can be overridden per-call with response_mode="object" to return {{ schema.class_name }}Object.
    """
    
    # raw_json=True returns RawAPIResponse regardless of response_mode
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }} | None = ...,
{% else %}
        name: str | None = ...,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
        *,
        raw_json: Literal[True],
        **kwargs: Any,
    ) -> RawAPIResponse: ...
    
    # Object mode override with mkey (single item)
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}
        name: str,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
        raw_json: bool = ...,
        *,
        response_mode: Literal["object"],
        **kwargs: Any,
{% if schema.fields %}
    ) -> {{ schema.class_name }}Object: ...
{% else %}
    ) -> FortiObject: ...
{% endif %}
    
    # Object mode override without mkey (list)
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: None = ...,
{% else %}
        name: None = ...,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
        raw_json: bool = ...,
        *,
        response_mode: Literal["object"],
        **kwargs: Any,
{% if schema.fields %}
{% if schema.mkey %}
    ) -> list[{{ schema.class_name }}Object]: ...
{% else %}
    ) -> {{ schema.class_name }}Object: ...
{% endif %}
{% else %}
{% if schema.mkey %}
    ) -> list[FortiObject]: ...
{% else %}
    ) -> FortiObject: ...
{% endif %}
{% endif %}
    
    # Dict mode with mkey (single item) - default
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}

# Response TypedDict for GET returns (all fields present in API response)
class {{ schema.class_name }}Response(TypedDict):
    """
    Type hints for {{ schema.path }} API response fields.
    
    All fields are present in the response from the FortiGate API.
    """
{% for field_name, field in schema.fields.items() %}
{% set field_snake = field_name|kebab_to_snake %}
{% if field_snake not in ['class', 'def', 'import', 'from', 'global', 'return', 'if', 'elif', 'else', 'for', 'while', 'with', 'try', 'except', 'finally', 'raise', 'assert', 'pass', 'break', 'continue', 'lambda', 'yield'] %}
{# Build comment with help text, default value, and validation hints #}
{% set comment_parts = [] %}
{% if field.help %}{% set _ = comment_parts.append(field.help|sanitize_comment(50)) %}{% endif %}
{% if field.default is not none and field.default != '' %}{% set _ = comment_parts.append('Default: ' ~ field.default) %}{% endif %}
{% if field.min_value is not none %}{% set _ = comment_parts.append('Min: ' ~ field.min_value) %}{% endif %}
{% if field.max_value is not none %}{% set _ = comment_parts.append('Max: ' ~ field.max_value) %}{% endif %}
{% if field.max_length is not none %}{% set _ = comment_parts.append('MaxLen: ' ~ field.max_length) %}{% endif %}
{% set field_comment = comment_parts|join(' | ') if comment_parts else '' %}
{% if field.options %}
    {{ field_snake }}: Literal[{{ field.options|map('tojson')|join(', ') }}]{% if field_comment %}  # {{ field_comment }}{% endif %}

{% elif field.type == 'integer' %}
    {{ field_snake }}: int{% if field_comment %}  # {{ field_comment }}{% endif %}

{% elif field.type == 'option' and field.enum_values %}
    {{ field_snake }}: Literal[{{ field.enum_values|map('tojson')|join(', ') }}]{% if field_comment %}  # {{ field_comment }}{% endif %}

{% elif field.is_list %}
{% if field.category == 'table' and field.children %}
{% set child_typeddict_name = schema.class_name + field_snake|title|replace('_', '') + 'Item' %}
    {{ field_snake }}: list[{{ child_typeddict_name }}]{% if field_comment %}  # {{ field_comment }}{% endif %}

{% else %}
    {{ field_snake }}: list[dict[str, Any]]{% if field_comment %}  # {{ field_comment }}{% endif %}

{% endif %}
{% else %}
    {{ field_snake }}: str{% if field_comment %}  # {{ field_comment }}{% endif %}

{% endif %}
{% endif %}
{% endfor %}


@final
class {{ schema.class_name }}Object:
    """Typed FortiObject for {{ schema.path }} with IDE autocomplete support.
    
    This is a typed wrapper that provides IDE autocomplete for API response fields.
    At runtime, this is actually a FortiObject instance.
    """
    
{% for field_name, field in schema.fields.items() %}
{% set field_snake = field_name|kebab_to_snake %}
{% if field_snake not in ['class', 'def', 'import', 'from', 'global', 'return', 'if', 'elif', 'else', 'for', 'while', 'with', 'try', 'except', 'finally', 'raise', 'assert', 'pass', 'break', 'continue', 'lambda', 'yield'] %}
{# Build comment with help text, default value, and validation hints #}
{% set comment_parts = [] %}
{% if field.help %}{% set _ = comment_parts.append(field.help|sanitize_comment(60)) %}{% endif %}
{% if field.default is not none and field.default != '' %}{% set _ = comment_parts.append('Default: ' ~ field.default) %}{% endif %}
{% if field.min_value is not none %}{% set _ = comment_parts.append('Min: ' ~ field.min_value) %}{% endif %}
{% if field.max_value is not none %}{% set _ = comment_parts.append('Max: ' ~ field.max_value) %}{% endif %}
{% if field.max_length is not none %}{% set _ = comment_parts.append('MaxLen: ' ~ field.max_length) %}{% endif %}
{% set field_comment = comment_parts|join(' | ') if comment_parts else field_name %}
    # {{ field_comment }}
{% if field.options %}
    {{ field_snake }}: Literal[{{ field.options|map('tojson')|join(', ') }}]
{% elif field.type == 'integer' %}
    {{ field_snake }}: int
{% elif field.type == 'option' and field.enum_values %}
    {{ field_snake }}: Literal[{{ field.enum_values|map('tojson')|join(', ') }}]
{% elif field.category == 'table' %}
{% if field.children %}
{% set child_class_name = schema.class_name + field_snake|title|replace('_', '') + 'Object' %}
    {{ field_snake }}: list[{{ child_class_name }}]
{% else %}
    {{ field_snake }}: list[FortiObject]
{% endif %}
{% elif field.is_list %}
    {{ field_snake }}: list[dict[str, Any]]
{% else %}
    {{ field_snake }}: str
{% endif %}
{% endif %}
{% endfor %}
    
    # Common API response fields
    status: str
    http_status: int | None
    vdom: str | None
    
    # Methods from FortiObject
    def get_full(self, name: str) -> Any: ...
    def to_dict(self) -> {{ schema.class_name }}Payload: ...
    def keys(self) -> Any: ...
    def values(self) -> Generator[Any, None, None]: ...
    def items(self) -> Generator[tuple[str, Any], None, None]: ...
    def get(self, key: str, default: Any = None) -> Any: ...
    def __getitem__(self, key: str) -> Any: ...

{% endif %}

class {{ schema.class_name }}:
    """
    {{ schema.help if schema.help else schema.path + ' endpoint' }}
    
    Path: {{ schema.path }}
    Category: {{ schema.category }}
{% if schema.mkey %}
    Primary Key: {{ schema.mkey }}
{% endif %}
    """
    
    # ================================================================
    # DEFAULT MODE OVERLOADS (no response_mode) - MUST BE FIRST
    # These match when response_mode is NOT passed (client default is "dict")
    # Pylance matches overloads top-to-bottom, so these must come first!
    # ================================================================
    
    # Default mode: mkey as positional arg -> returns typed dict
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}
        name: str,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
{% if schema.fields %}
    ) -> {{ schema.class_name }}Response: ...
{% else %}
    ) -> dict[str, Any]: ...
{% endif %}
    
    # Default mode: mkey as keyword arg -> returns typed dict
    @overload
    def get(
        self,
        *,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}
        name: str,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
{% if schema.fields %}
    ) -> {{ schema.class_name }}Response: ...
{% else %}
    ) -> dict[str, Any]: ...
{% endif %}
    
    # Default mode: no mkey -> returns list of typed dicts
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: None = None,
{% else %}
        name: None = None,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
{% if schema.fields %}
{% if schema.mkey %}
    ) -> list[{{ schema.class_name }}Response]: ...
{% else %}
    ) -> {{ schema.class_name }}Response: ...
{% endif %}
{% else %}
{% if schema.mkey %}
    ) -> list[dict[str, Any]]: ...
{% else %}
    ) -> dict[str, Any]: ...
{% endif %}
{% endif %}
    
    # ================================================================
    # EXPLICIT response_mode="object" OVERLOADS
    # ================================================================
    
    # Object mode: mkey as positional arg -> returns single object
    @overload
    def get(
        self,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}
        name: str,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
        raw_json: Literal[False] = ...,
        *,
        response_mode: Literal["object"],
        **kwargs: Any,
{% if schema.fields %}
    ) -> {{ schema.class_name }}Object: ...
{% else %}
    ) -> FortiObject: ...
{% endif %}
    
    # Object mode: mkey as keyword arg -> returns single object
    @overload
    def get(
        self,
        *,
{% if schema.mkey %}
        {{ schema.mkey|kebab_to_snake }}: {{ schema.mkey_type|to_python_type }},
{% else %}
        name: str,
{% endif %}
        filter: str | list[str] | None = ...,
        count: int | None = ...,
        start: int | None = ...,
        payload_dict: dict[str, Any] | None = ...,
        range: list[int] | None = ...,
        sort: str | None = ...,
        format: str | None = ...,
        action: str | None = ...,
        vdom: str | bool | None = ...,
        raw_json: Literal[False] = ...,
        response_mode: Literal["object"],
        **kwargs: Any,
{% if schema.fields %}
    ) -> {{ schema.class_name }}Object: ...
{% else %}
    ) -> FortiObject: ...
{% endif %}
    

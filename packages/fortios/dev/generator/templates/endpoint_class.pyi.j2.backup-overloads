# ============================================================================
# Payload TypedDict for IDE autocomplete (for POST/PUT - fields are optional)
# ============================================================================
# NOTE: We intentionally DON'T use NotRequired wrapper because:
# 1. total=False already makes all fields optional
# 2. NotRequired[Literal[...]] prevents Pylance from validating Literal values in dict literals
class {{ schema.class_name }}Payload(TypedDict, total=False):
    """
    Type hints for {{ schema.path }} payload fields.
    
    {{ schema.help if schema.help else 'Configure ' + schema.path }}
    
{% if relationship_section %}
    {{ relationship_section }}
{% endif %}
    **Usage:**
        payload: {{ schema.class_name }}Payload = {
            "field": "value",  # <- autocomplete shows all fields
        }
    """
{% for field_name, field in schema.fields.items() %}
{% set field_snake = field_name|kebab_to_snake %}
{% if field_snake not in ['class', 'def', 'import', 'from', 'global', 'return', 'if', 'elif', 'else', 'for', 'while', 'with', 'try', 'except', 'finally', 'raise', 'assert', 'pass', 'break', 'continue', 'lambda', 'yield'] %}
{# Build comment with help text, default value, and validation hints #}
{% set comment_parts = [] %}
{% if field.help %}{% set _ = comment_parts.append(field.help|sanitize_comment(50)) %}{% endif %}
{% if field.default is not none and field.default != '' %}{% set _ = comment_parts.append('Default: ' ~ field.default|sanitize_default) %}{% endif %}
{% if field.min_value is not none %}{% set _ = comment_parts.append('Min: ' ~ field.min_value) %}{% endif %}
{% if field.max_value is not none %}{% set _ = comment_parts.append('Max: ' ~ field.max_value) %}{% endif %}
{% if field.max_length is not none %}{% set _ = comment_parts.append('MaxLen: ' ~ field.max_length) %}{% endif %}
{% set field_comment = comment_parts|join(' | ') if comment_parts else field_name %}
{# Check if field has enum options - NO NotRequired wrapper for Literal validation to work #}
{% if field.options %}
    {{ field_snake }}: Literal[{{ field.options|map('tojson')|join(', ') }}]  # {{ field_comment }}
{% elif field.type in ['integer', 'int'] %}
    {{ field_snake }}: int  # {{ field_comment }}
{% elif field.type == 'boolean' %}
    {{ field_snake }}: bool  # {{ field_comment }}
{% elif field.type == 'option' and field.enum_values %}
    {{ field_snake }}: Literal[{{ field.enum_values|map('tojson')|join(', ') }}]  # {{ field_comment }}
{% elif field.category == 'table' and field.children %}
{% set child_typeddict_name = schema.class_name + field_snake|title|replace('_', '') + 'Item' %}
    {{ field_snake }}: list[{{ child_typeddict_name }}]  # {{ field_comment }}
{% elif field.is_list %}
    {{ field_snake }}: list[dict[str, Any]]  # {{ field_comment }}
{% else %}
    {{ field_snake }}: str  # {{ field_comment }}
{% endif %}
{% endif %}
{% endfor %}


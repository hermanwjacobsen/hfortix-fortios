{% for field_name, field in schema.fields.items() %}
{% if field.category == 'table' and field.children %}
{% set field_snake = field_name|kebab_to_snake %}
{% set child_typeddict_name = schema.class_name + field_snake|title|replace('_', '') + 'Item' %}

class {{ child_typeddict_name }}(TypedDict, total=False):
    """Type hints for {{ field_name }} table item fields (dict mode).
    
    Provides IDE autocomplete for nested table field items.
    Use this when building payloads for POST/PUT requests.
    
    **Available fields:**
{% for child_name, child_field in field.children.items() %}
{% set child_snake = child_name|kebab_to_snake %}
{% if child_snake not in ['class', 'def', 'import', 'from', 'global', 'return', 'if', 'elif', 'else', 'for', 'while', 'with', 'try', 'except', 'finally', 'raise', 'assert', 'pass', 'break', 'continue', 'lambda', 'yield'] %}
        - {{ child_snake }}{% if child_field.options %}: {{ child_field.options|map('tojson')|join(' | ') }}{% elif child_field.type in ['integer', 'int'] %}: int{% elif child_field.type == 'boolean' %}: bool{% else %}: str{% endif %}

{% endif %}
{% endfor %}
    
    **Example:**
        entry: {{ child_typeddict_name }} = {
            "status": "enable",  # <- autocomplete shows all fields and validates Literal values
        }
    """
    
{% for child_name, child_field in field.children.items() %}
{% set child_snake = child_name|kebab_to_snake %}
{% if child_snake not in ['class', 'def', 'import', 'from', 'global', 'return', 'if', 'elif', 'else', 'for', 'while', 'with', 'try', 'except', 'finally', 'raise', 'assert', 'pass', 'break', 'continue', 'lambda', 'yield'] %}
{# Build comment with help text, default value, and validation hints #}
{% set comment_parts = [] %}
{% if child_field.help %}{% set _ = comment_parts.append(child_field.help|sanitize_comment(50)) %}{% endif %}
{% if child_field.default is not none and child_field.default != '' %}{% set _ = comment_parts.append('Default: ' ~ child_field.default|sanitize_default) %}{% endif %}
{% if child_field.min_value is not none %}{% set _ = comment_parts.append('Min: ' ~ child_field.min_value) %}{% endif %}
{% if child_field.max_value is not none %}{% set _ = comment_parts.append('Max: ' ~ child_field.max_value) %}{% endif %}
{% if child_field.max_length is not none %}{% set _ = comment_parts.append('MaxLen: ' ~ child_field.max_length) %}{% endif %}
{% set field_comment = comment_parts|join(' | ') if comment_parts else child_name %}
{% if child_field.options %}
    {{ child_snake }}: Literal[{{ child_field.options|map('tojson')|join(', ') }}]  # {{ field_comment }}
{% elif child_field.type in ['integer', 'int'] %}
    {{ child_snake }}: int  # {{ field_comment }}
{% elif child_field.type == 'option' and child_field.enum_values %}
    {{ child_snake }}: Literal[{{ child_field.enum_values|map('tojson')|join(', ') }}]  # {{ field_comment }}
{% else %}
    {{ child_snake }}: str  # {{ field_comment }}
{% endif %}
{% endif %}
{% endfor %}

{% endif %}

class {{ schema.class_name }}:
    """
    {{ schema.help if schema.help else schema.path + ' endpoint' }}
    
    Path: {{ schema.path }}
    Category: {{ schema.category }}
{% if schema.mkey %}
    Primary Key: {{ schema.mkey }}
{% endif %}
    """
    
    def __init__(self, client: Any) -> None:
        """Initialize endpoint with HTTP client.
        
        Args:
            client: HTTP client instance for API communication
        """
        ...
    
    # ================================================================
    # GET OVERLOADS - Always returns FortiObject (or ContentResponse for file endpoints)
    # Pylance matches overloads top-to-bottom, so these must come first!
    # ================================================================
    
{% if is_content_endpoint %}
    # Content endpoint - returns ContentResponse (binary/text content)
    @overload
    def get(
        self,
        *,
{% if query_params and query_params.GET %}
{% for param_name, param_info in query_params.GET.items() %}
{% set param_snake = param_name|kebab_to_snake %}
{% set param_type = 'int' if param_info.type in ['integer', 'int'] else 'str' %}
{% if param_info.allowed_values %}
{% if param_info.required %}
        {{ param_snake }}: Literal[{{ param_info.allowed_values|map('tojson')|join(', ') }}],
{% else %}
        {{ param_snake }}: Literal[{{ param_info.allowed_values|map('tojson')|join(', ') }}] | None = ...,
{% endif %}
{% else %}
{% if param_info.required %}
        {{ param_snake }}: {{ param_type }},
{% else %}
        {{ param_snake }}: {{ param_type }} | None = ...,
{% endif %}
{% endif %}
{% endfor %}
{% endif %}
{% if not query_params or not query_params.GET or 'filter' not in query_params.GET %}
        filter: str | list[str] | None = ...,
{% endif %}
{% if not query_params or not query_params.GET or 'count' not in query_params.GET %}
        count: int | None = ...,
{% endif %}
{% if not query_params or not query_params.GET or 'start' not in query_params.GET %}
        start: int | None = ...,
{% endif %}
        payload_dict: dict[str, Any] | None = ...,
{% if not schema.is_global_only %}
        vdom: str | bool | None = ...,
{% endif %}
        error_mode: Literal["raise", "return", "print"] | None = ...,
        error_format: Literal["detailed", "simple", "code_only"] | None = ...,
    ) -> ContentResponse: ...
    
{% elif query_params and query_params.GET and schema.category in ['service', 'monitor'] %}
    # Service/Monitor endpoint with query parameters
    @overload
    def get(
        self,
        *,
{% for param_name, param_info in query_params.GET.items() %}
{% set param_snake = param_name|kebab_to_snake %}
{% set param_type = 'int' if param_info.type in ['integer', 'int'] else 'str' %}
{% if param_info.allowed_values %}
{% if param_info.required %}

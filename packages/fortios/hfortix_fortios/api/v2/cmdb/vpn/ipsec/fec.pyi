# Nested classes for table field children

@final
class FecMappingsObject:
    """Typed object for mappings table items.
    
    Provides IDE autocomplete for nested table field attributes.
    At runtime, this is a FortiObject instance.
    """
    
    # Sequence number (1 - 64).
    seqno: int
    # Number of base FEC packets (1 - 20).
    base: int
    # Number of redundant FEC packets (1 - 5).
    redundant: int
    # Apply FEC parameters when packet loss is >= threshold
    packet_loss_threshold: int
    # Apply FEC parameters when latency is <= threshold (0 means no threshold).
    latency_threshold: int
    # Apply FEC parameters when available up bandwidth is >= threshold
    bandwidth_up_threshold: int
    # Apply FEC parameters when available down bandwidth is >= threshold
    bandwidth_down_threshold: int
    # Apply FEC parameters when available bi-bandwidth is >= threshold
    bandwidth_bi_threshold: int
    
    # Methods from FortiObject
    def get_full(self, name: str) -> Any: ...
    def to_dict(self) -> dict[str, Any]: ...
    def keys(self) -> Any: ...
    def values(self) -> Generator[Any, None, None]: ...
    def items(self) -> Generator[tuple[str, Any], None, None]: ...
    def get(self, key: str, default: Any = None) -> Any: ...
    def __getitem__(self, key: str) -> Any: ...



